{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "odatalib/**/*",
          "webapi/v6/*",
          "client/v6/*"
        ]
      },
      {
        "files": [
          "*.{md,yml}"
        ],
        "src": "odatalib/v6",
        "dest": "odatalib"
      },
      {
        "files": [
          "*.{md,yml}"
        ],
        "src": "odatalib/v7",
        "dest": "odatalib"
      },
      {
        "files": [
          "*.{md,yml}"
        ],
        "src": "odatalib/v7/edm",
        "dest": "odatalib/edm"
      },
      {
        "files": [
          "*.{md,yml}"
        ],
        "src": "client/v6",
        "dest": "client"
      },
      {
        "files": [
          "*.{md,yml}"
        ],
        "src": "webapi/v6",
        "dest": "webapi"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.service": "odata",
      "breadcrumb_path": "/odata/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None", 
      "ms.author":"saumadan",
      "ms.topic": "conceptual",
      "uhfHeaderId": "MSDocsHeader-OData",
      "titleSuffix": "OData",
      "searchScope": ["OData"]
    },
    "fileMetadata": {
      "titleSuffix": {
        "/odata/client/**/*.md": "OData Client",
        "/odata/odatalib/**/*.md": "OData Core library",
        "/odata/webapi/**/*.md": "OData AspNet WebAPI",
        "/odata/spatial/**/*.md": "Microsoft Spatial",
        "/odata/concepts/**/*.md": "OData .NET",
        "/odata/connectedservice/**/*.md": "OData Connected Service"
      }
    },
    "template": [],
    "dest": "OData-conceptual",
    "markdownEngineName": "markdig"
  }
}
